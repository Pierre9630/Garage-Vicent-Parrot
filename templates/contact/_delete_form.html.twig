<form method="post" action="{{ path('app_contact_delete', {'id': contact.id}) }}" onsubmit="return confirm('Voulez vous supprimer cette demande de contact?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contact.id) }}">
    <button class="btn"><i class="fa-solid fa-trash"></i></button>
</form>
